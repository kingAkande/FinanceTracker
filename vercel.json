{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/__/auth/handler",
      "destination": "https://financetracker-fb7b7.firebaseapp.com/__/auth/handler"
    }
  ]
}
